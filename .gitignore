# Ignore bundler config.
/app-server/.bundle

# Ignore all logfiles and tempfiles.
/app-server/log/*
!/app-server/log/.keep
/app-server/tmp

# IDE
/app-server/.idea

# installed by Gemfile / Bowerfile
/app-server/vendor/bundle
/app-server/vendor/assets/bower_components
